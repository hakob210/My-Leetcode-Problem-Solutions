class Solution(object):
    def reverseWords(self, s):
        """
        :type s: str
        :rtype: str
        """
        l = []
        word = ""

        for i in s:
            if i != " ":
                word += i
            else:
                l.append(word)
                word = ""

        return l










sol = Solution()
res = sol.reverseWords("rythm. no got have feet guilty again, dance gonna never I'm")
print(res)
