class Solution(object):
    def countSubarrays(self, nums, minK, maxK):
        """
        :type nums: List[int]
        :type minK: int
        :type maxK: int
        :rtype: int
        """
        def countSubarraysInRange(nums, k):
            count = 0
            result = 0
            left = 0
            
            for right in range(len(nums)):
                if nums[right] <= k:
                    count += 1
                while left <= right and nums[left] > k:
                    left += 1
                    count = 0
                result += count
            return result

        return countSubarraysInRange(nums, maxK) - countSubarraysInRange(nums, minK - 1)








sol = Solution()
res = sol.countSubarrays([1, 1, 1, 1], 1, 1)
print(res)

