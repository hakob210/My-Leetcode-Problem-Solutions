class Solution(object):
    def makeGood(self, s):
        """
        :type s: str
        :rtype: str
        """
        l = []

        for i in range(len(s)-2):
            char = s[i]
            nextChar = s[i+1]
            if char != nextChar and char.lower() == nextChar.lower():
                l.append(char)
                l.append(nextChar)


        return l










sol = Solution()
res1 = sol.makeGood("leEeetcode")
res2 = sol.makeGood("abBAcC")
print(res1, res2)
